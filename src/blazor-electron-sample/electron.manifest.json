{
  "executable": "blazor-electron-sample",
  "splashscreen": {
    "imageFile": ""
  },
  "name": "blazor-electron-sample",
  "author": "",
  "singleInstance": false,
  "environment": "Production",
  "build": {
    "appId": "com.blazor-electron-sample.app",
    "productName": "blazor-electron-sample",
    "copyright": "Copyright Â© 2020",
    "buildVersion": "1.0.0",
    "compression": "maximum",
    "directories": {
      "output": "../../../bin/Desktop"
    },
    "extraResources": [
      {
        "from": "./bin",
        "to": "bin",
        "filter": [ "**/*" ]
      }
    ],
    "files": [
      "**/*"
    ],
    "mac": {
      "target": "dmg",
      "category": "public.app-category.business"
    },
    "linux": {
      "target": ["AppImage","snap","deb", "rpm"],
      "vendor": "bravecobra.com"
    },
    "win": {
      "target": "nsis",
      "msi": false,
      "oneClick": true,
      "perMachine": false,
      "icon": "Assets/electron.ico"
    }
  }
}